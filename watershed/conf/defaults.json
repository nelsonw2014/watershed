{
  "AWS": {
    "CloudWatch":{
      "alarmOnIdle": true,
      "alarmAfter": 120,
      "alarmActions": ["arn:aws:sns:us-east-1:<ACCOUNT>:<SNS_TOPIC>"]
    },
    "S3": {
      "accessKey": "accessKey",
      "secretKey": "secretKey",
      "resourcesBucket": "watershed",
      "resourcesPrefix": "resources"
    },
    "EMR": {
      "clusterName": "watershed",
      "roles": {
        "service": "EMR_DefaultRole",
        "ec2": "EMR_EC2_DefaultRole"
      },
      "ec2KeyName": null,
      "ec2SubnetId": null,
      "additionalMasterSecurityGroups": [],
      "amiVersion": "3.8.0",
      "instanceGroups": [
        {
          "Name": "EMR Master",
          "InstanceRole": "MASTER",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        },
        {
          "Name": "EMR Core",
          "InstanceRole": "CORE",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        },
        {
          "Name": "EMR Task",
          "InstanceRole": "TASK",
          "InstanceCount": 1,
          "InstanceType": "m3.xlarge"
        }
      ],
      "tags": [
        {
          "Key": "Name",
          "Value": "watershed"
        }
      ]
    },
    "profile": "default",
    "streams": [
      {
        "table": "app_event",
        "stream": "app_event_stream"
      }
    ],
    "archives": [
      {
        "dfsUrl": "s3://AccessKey:SecretKey@watershed",
        "path": "/stream-archives",
        "name": "app_event"
      }
    ]
  }
}